{
  "cli": {
    "version": ">= 16.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASE": "http://localhost:8080"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASE": "https://ajo-app-22jy.onrender.com"
      }
    },
    "production": {
      "env": {
        "API_BASE": "https://ajo-app-22jy.onrender.com"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
